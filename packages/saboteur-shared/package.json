{
  "name": "saboteur-shared",
  "version": "1.0.0",
  "scripts": {
    "start": "watch-run -p 'src/**/*.js' npm run compile",
    "install": "npm clean && npm run compile",
    "clean": "rm -fr out lib",
    "compile": "babel src --presets=es2015 --out-dir dist"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "watch-run": "^1.2.5"
  }
}
